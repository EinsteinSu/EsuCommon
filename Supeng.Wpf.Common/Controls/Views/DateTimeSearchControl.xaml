<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" x:Class="Supeng.Wpf.Common.Controls.Views.DateTimeSearchControl" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <TextBlock Text="{Binding DateTitle}" Margin="5" />
        <dxe:DateEdit EditValue="{Binding StartDate,Mode=TwoWay}"/>
        <TextBlock Text="至" Margin="5"/>
        <dxe:DateEdit EditValue="{Binding EndDate,Mode=TwoWay}"/>
    </StackPanel>
</UserControl>
