<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" x:Class="Supeng.Wpf.Common.DialogWindows.Views.ListEditControl" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <dxe:ListBoxEdit ItemsSource="{Binding Data.Collection,Mode=TwoWay}" SelectedItem="{Binding Data.CurrentItem}"/>
    </Grid>
</UserControl>
