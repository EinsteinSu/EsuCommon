<UserControl x:Class="Supeng.Silverlight.Controls.Text.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:views="clr-namespace:Supeng.Silverlight.Controls.Views;assembly=Supeng.Silverlight.Controls"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <Grid Background="White">
        <views:PreviewTextControl Text="{Binding Text,Mode=TwoWay}"/>
    </Grid>
</UserControl>
